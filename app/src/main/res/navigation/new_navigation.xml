<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/new_navigation"
    app:startDestination="@id/episodesFragment2"
    >
    <fragment
        android:id="@+id/episodesFragment2"
        android:name="com.example.finalfinalspace.fragments.EpisodesFragment"
        android:label="@string/episodes"
        tools:layout="@layout/fragment_episodes"
        >
        <action
            android:id="@+id/action_episodesFragment2_to_episodeFragment2"
            app:destination="@id/episodeFragment2"
            />
    </fragment>
    <fragment
        android:id="@+id/quotesFragment2"
        android:name="com.example.finalfinalspace.fragments.QuotesFragment"
        android:label="@string/quotes"
        tools:layout="@layout/fragment_quotes"
        />
    <fragment
        android:id="@+id/episodeFragment2"
        android:name="com.example.finalfinalspace.fragments.EpisodeFragment"
        android:label="@string/episodes"
        tools:layout="@layout/fragment_episode"
        />
</navigation>